@using System.Net.Mime
@model IEnumerable<digioz.Portal.Bo.ViewModels.RSSViewModel>

@if (Model.Any())
{
    foreach (var feed in Model)
    {
        <div class="card bg-light mb-3">
            <div class="card-header">@feed.Title</div>
            <div class="card-body">
                @Html.Raw(feed.Body)

                <label style="font-size:8px;">@feed.PublishDate</label>
            </div>
        </div>
    }
}